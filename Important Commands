sysctl -a --pattern "randomize"                    ---->TO check the value
sysctl -w kernel.randomize_va_space=0              ----->TO change the value
cat  /proc/<pid>/maps

ps -e -o ppid,pid,cmd,class,ni,rtprio,vsz,rsz  


ps -em -o pid,ppid,cmd,vsz,rsz,nlwp,lwp,stat,class,ni,rtprio,psr ....| less-->To test the processes and their thread states

sh nameofScript.sh     //To execute the script
